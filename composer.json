{
	"description": "A template library for Mastodon bots.",
	"homepage": "https://github.com/php-tootbot/php-tootbot",
	"license": "MIT",
	"type": "project",
	"authors": [
		{
			"name": "Contributors",
			"homepage":"https://github.com/php-tootbot/php-tootbot/graphs/contributors"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "^8.1",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-openssl": "*",
		"php-tootbot/php-tootbot": "dev-main#84405c245a458780ca691e4249f8197fb27ca4c1"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.0",
		"phan/phan": "^5.4"
	},
	"autoload": {
		"psr-4": {
			"PHPTootBot\\MyTootBot\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PHPTootBot\\MyTootBotTest\\": "tests/"
		}
	},
	"scripts": {
		"phpunit": "@php vendor/bin/phpunit",
		"phan": "@php vendor/bin/phan"
	},
	"config": {
		"lock": true,
		"sort-packages": true,
		"platform-check": true
	}
}
